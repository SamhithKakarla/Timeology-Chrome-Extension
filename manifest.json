{
    "manifest_version": 2,
    "name": "Timeology",
    "version": "1.0",
    "description": "Timeology helps you organize your time.",
    "permissions": [
        "declarativeContent",
        "storage",
        "activeTab"
    ], 
    
    "page_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/58e8fe8deb97430e819064c7.png",
            "32": "images/58e8fe8deb97430e819064c7.png",
            "48": "images/58e8fe8deb97430e819064c7.png",
            "128": "images/58e8fe8deb97430e819064c7.png"
        }
    },
    "icons": {
        "16": "images/58e8fe8deb97430e819064c7.png",
        "32": "images/58e8fe8deb97430e819064c7.png",
        "48": "images/58e8fe8deb97430e819064c7.png",
        "128": "images/58e8fe8deb97430e819064c7.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://fuhsd.schoology.com/"
            ],
            "js": [
                "Script/test.js"
            ],
            "run_at": "document_end"
        }
    ]
}